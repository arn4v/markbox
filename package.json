	"scripts": {
		"start": "next start -p ${PORT-3000}",
		"start-ci": "yarn db-migrate-deploy && yarn start",
		"build": "next build",
		"build-start": "yarn build && yarn start",
		"dev": "concurrently -n \"next,prisma-gen\" \"next dev\" \"npm run db-gen-watch\"",
		"format": "prettier --write ./src/**/*.{js,jsx,ts,tsx}",
		"format-imports": "npm run format -- --plugin prettier-plugin-organize-imports",
		"gen": "npm run db-gen",
		"prisma": "prisma",
		"db": "prisma studio --port 5556 --browser none",
		"db-gen": "prisma generate",
		"db-gen-watch": "nodemon -e prisma -w ./prisma --exec prisma generate && prisma db push",
		"db-push": "prisma db push --skip-generate",
		"db-seed-users": "prisma db seed --preview-feature",
		"db-seed-bookmarks": "node ./scripts/seed-bookmarks.mjs",
		"db-dev": "nodemon -w ./prisma -e prisma --exec npx prisma generate && npx prisma db push",
		"db-migrate-create": "prisma migrate dev --create-only --skip-generate --skip-seed",
		"db-migrate-dev": "prisma migrate dev --skip-generate",
		"db-migrate-deploy": "prisma migrate deploy",
		"lint": "next lint",
		"lint-fix": "next lint --fix",
		"start-docker": "docker-compose up -d",
		"stop-docker": "docker-compose down",
		"add-user": "node ./scripts/add-user.ts",
		"cy-open": "cypress open",
		"cy-run": "cypress run --browser chrome",
		"cy-test": "start-server-and-test 3000 cy-run",
		"cy-test-ui": "start-server-and-test 3000 cy-open",
		"typecheck": "tsc --noEmit"
	},
