{
  "name": "bookmarky",
  "version": "0.1.0",
  "license": "AGPL-3.0-or-later",
  "author": "Arnav Gosain <arnav@arnavgosain.com>",
  "scripts": {
    "start": "next start",
    "build": "next build",
    "dev": "concurrently -n \"next,gql-codegen,prisma-gen\" \"next dev\" \"npm run gql-gen-watch\" \"npm run db-gen-watch\"",
    "format": "prettier --write ./src/**/*.{js,jsx,ts,tsx}",
    "gen": "npm run gql-gen && npm run db-gen",
    "gen-watch": "concurrently 'npm run gql-gen' 'npm run db-gen'",
    "gql-gen": "graphql-codegen",
    "gql-gen-watch": "graphql-codegen --watch 'src/graphql/**/*.ts'",
    "db": "prisma studio --port 5556 --browser none",
    "db-gen": "prisma generate",
    "db-gen-watch": "prisma generate --watch",
    "db-push": "prisma db push --skip-generate",
    "db-seed-users": "prisma db seed --preview-feature",
    "db-dev": "nodemon -w ./prisma -e prisma --exec npx prisma generate && npx prisma db push",
    "db-migrate-create": "prisma migrate dev --create-only",
    "db-migrate-dev": "prisma migrate dev",
    "db-migrate-deploy": "prisma migrate deploy",
    "db-migrate": "prisma db migrate deploy"
  },
  "dependencies": {
    "@apollo/client": "^3.3.20",
    "@next/bundle-analyzer": "^10.2.3",
    "@prisma/client": "^2.24.1",
    "@types/uuid": "^8.3.0",
    "apollo-server-micro": "^2.25.1",
    "bcrypt": "^5.0.1",
    "clsx": "^1.1.1",
    "concurrently": "^6.2.0",
    "cookie": "^0.4.1",
    "date-fns": "^2.22.1",
    "fast-jwt": "^1.1.3",
    "formik": "^2.2.9",
    "framer-motion": "^4.1.17",
    "graphql": "^15.5.0",
    "graphql-tools": "^7.0.5",
    "inter-ui": "^3.18.1",
    "ms": "^2.1.3",
    "next": "^10.2.3",
    "next-offline": "^5.0.5",
    "next-themes": "^0.0.14",
    "nodemon": "^2.0.7",
    "password-validator": "^5.1.1",
    "react": "^17.0.2",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-modal": "^3.14.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^2.0.2",
    "@graphql-codegen/cli": "^1.21.5",
    "@graphql-codegen/typescript": "^1.22.1",
    "@graphql-codegen/typescript-operations": "^1.18.0",
    "@graphql-codegen/typescript-react-apollo": "^2.2.5",
    "@graphql-codegen/typescript-resolvers": "^1.19.2",
    "@tailwindcss/forms": "^0.3.3",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie": "^0.4.0",
    "@types/ms": "^0.7.31",
    "@types/node": "^15.12.2",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "@types/react-redux": "^7.1.16",
    "autoprefixer": "^10.2.6",
    "postcss": "^8.3.4",
    "prettier": "^2.3.1",
    "prisma": "^2.24.1",
    "tailwindcss": "^2.1.4",
    "typescript": "^4.3.2"
  }
}
