{
	"name": "bookmarky",
	"version": "0.1.0",
	"license": "AGPL-3.0-or-later",
	"author": "Arnav Gosain <arnav@arnavgosain.com>",
	"scripts": {
		"start": "next start",
		"build": "next build",
		"dev": "concurrently -n \"next,gql-codegen,prisma-gen\" \"next dev\" \"npm run gql-gen-watch\" \"npm run db-gen\"",
		"format": "prettier --write ./src/**/*.{js,jsx,ts,tsx}",
		"gen": "npm run gql-gen && npm run db-gen",
		"gen-watch": "concurrently 'npm run gql-gen' 'npm run db-gen'",
		"gql-gen": "graphql-codegen",
		"gql-gen-watch": "graphql-codegen --watch 'src/graphql/**/*.ts'",
		"prisma": "prisma",
		"db": "prisma studio --port 5556 --browser none",
		"db-gen": "prisma generate",
		"db-gen-watch": "prisma generate --watch",
		"db-push": "prisma db push --skip-generate",
		"db-seed-users": "prisma db seed --preview-feature",
		"db-dev": "nodemon -w ./prisma -e prisma --exec npx prisma generate && npx prisma db push",
		"db-migrate-create": "prisma migrate dev --create-only",
		"db-migrate-dev": "prisma migrate dev",
		"db-migrate-deploy": "prisma migrate deploy",
		"db-migrate": "prisma db migrate deploy",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook",
		"cy:run": "cypress run --headless --browser chrome",
		"cy:open": "cypress open --browser chrome",
		"test:e2e:ci": "start-server-and-test dev http://localhost:3000 cy:run",
		"start-docker": "docker-compose up -d"
	},
	"dependencies": {
		"@prisma/client": "^2.25.0",
		"@reach/dialog": "^0.15.0",
		"apollo-server-micro": "^2.25.1",
		"bcrypt": "^5.0.1",
		"clsx": "^1.1.1",
		"cookie": "^0.4.1",
		"date-fns": "^2.22.1",
		"fast-jwt": "^1.1.3",
