{
	"name": "Bookmarky",
	"short_name": "Bookmarky",
	"start_url": "/dashboard",
	"scope": "/",
	"orientation": "portrait",
	"display": "standalone",
	"background_color": "#FFFFFF",
	"theme_color": "#FFFFFF",
	"splash_pages": null,
	"share_target": {
		"action": "/api/create",
		"method": "GET",
		"enctype": "application/x-www-form-urlencoded",
		"params": {
			"title": "title",
			"url": "url",
			"text": "description"
		}
	},
	"icons": [
		{
			"src": "icons/manifest-icon-192.png",
			"sizes": "192x192",
			"type": "image/png",
			"purpose": "maskable any"
		},
		{
			"src": "icons/manifest-icon-512.png",
			"sizes": "512x512",
			"type": "image/png",
			"purpose": "maskable any"
		}
	]
}
